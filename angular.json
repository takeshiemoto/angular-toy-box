{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "dependency-injection-basic-usage",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "dependency-injection-basic-usage": {
      "projectType": "application",
      "root": "apps/dependency-injection-basic-usage",
      "sourceRoot": "apps/dependency-injection-basic-usage/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/dependency-injection-basic-usage",
            "index": "apps/dependency-injection-basic-usage/src/index.html",
            "main": "apps/dependency-injection-basic-usage/src/main.ts",
            "polyfills": "apps/dependency-injection-basic-usage/src/polyfills.ts",
            "tsConfig": "apps/dependency-injection-basic-usage/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/dependency-injection-basic-usage/src/favicon.ico",
              "apps/dependency-injection-basic-usage/src/assets"
            ],
            "styles": ["apps/dependency-injection-basic-usage/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/dependency-injection-basic-usage/src/environments/environment.ts",
                  "with": "apps/dependency-injection-basic-usage/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "dependency-injection-basic-usage:build:production"
            },
            "development": {
              "browserTarget": "dependency-injection-basic-usage:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dependency-injection-basic-usage:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dependency-injection-basic-usage/src/**/*.ts",
              "apps/dependency-injection-basic-usage/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dependency-injection-basic-usage"],
          "options": {
            "jestConfig": "apps/dependency-injection-basic-usage/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ssr-example": {
      "projectType": "application",
      "root": "apps/ssr-example",
      "sourceRoot": "apps/ssr-example/src",
      "prefix": "angular-toy-box",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ssr-example",
            "index": "apps/ssr-example/src/index.html",
            "main": "apps/ssr-example/src/main.ts",
            "polyfills": "apps/ssr-example/src/polyfills.ts",
            "tsConfig": "apps/ssr-example/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/ssr-example/src/favicon.ico",
              "apps/ssr-example/src/assets"
            ],
            "styles": ["apps/ssr-example/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ssr-example/src/environments/environment.ts",
                  "with": "apps/ssr-example/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ssr-example:build:production"
            },
            "development": {
              "browserTarget": "ssr-example:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ssr-example:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ssr-example/src/**/*.ts",
              "apps/ssr-example/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ssr-example"],
          "options": {
            "jestConfig": "apps/ssr-example/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
